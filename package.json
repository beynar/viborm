{
  "name": "baseorm",
  "version": "0.1.0",
  "description": "Type-safe ORM for PostgreSQL and MySQL with Prisma-like API",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "module": "dist/index.mjs",
  "exports": {
    ".": {
      "import": "./dist/index.mjs",
      "require": "./dist/index.js",
      "types": "./dist/index.d.ts"
    }
  },
  "scripts": {
    "build": "tsc",
    "dev": "tsc --watch",
    "test": "vitest",
    "test:ui": "vitest --ui",
    "type-check": "tsc --noEmit"
  },
  "keywords": [
    "orm",
    "typescript",
    "postgres",
    "mysql",
    "type-safe",
    "prisma-like"
  ],
  "author": "",
  "license": "ISC",
  "packageManager": "pnpm@10.11.0",
  "devDependencies": {
    "@types/node": "^22.15.21",
    "@vitest/ui": "^3.1.4",
    "prisma": "^6.8.2",
    "typescript": "^5.8.3",
    "vitest": "^3.1.4"
  },
  "dependencies": {
    "@prisma/client": "6.8.2",
    "drizzle-orm": "^0.43.1"
  }
}
